CREATE OR ALTER TRIGGER PR_PROTOKOLY_TR_BI FOR PR_PROTOKOLY
ACTIVE BEFORE  INSERT POSITION 0
AS

begin
 IF (NEW.ID_PROTOKOLU IS NULL OR NEW.ID_PROTOKOLU <1 ) THEN
 NEW.ID_PROTOKOLU=GEN_ID(PR_PROTOKOLY_GEN,1);
end
#C

ALTER TABLE PR_PROTOKOLY
add PRZYJAL Varchar(200),
add PRZEKAZAL Varchar(200)
#TRY

UPDATE PR_PROTOKOLY SET PRZYJAL = NULL
#C

UPDATE PR_PROTOKOLY SET PRZEKAZAL = NULL
#C
