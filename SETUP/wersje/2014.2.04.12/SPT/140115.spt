ALTER TABLE PR_DELEGACJE add ROK Integer NOT NULL
#TRY

ALTER TABLE PR_DELEGACJE add DNI Integer NOT NULL
#TRY

UPDATE PR_DELEGACJE SET 
ROK = CASE WHEN ROK IS NULL THEN EXTRACT(YEAR FROM CAST('NOW' AS DATE)) ELSE ROK END,
DNI = CASE WHEN DNI IS NULL THEN 0 ELSE DNI END
#C

ALTER TABLE PR_DELEGACJE ADD CONSTRAINT PR_DELEGACJE_FK_MIESIACE FOREIGN KEY (ROK,MIESIAC) REFERENCES PR_MIES4RBH(ROK,NR_MIES) ON UPDATE CASCADE ON DELETE CASCADE
#TRY

