DELETE FROM PR_ZLECENIA Z WHERE EXISTS (SELECT NULL FROM PR_KOMORKI K INNER JOIN PR_WYDZIALY W ON W.ID_WYDZIALU = K.ID_WYDZIALU WHERE K.ID_KOMORKI = Z.ID_KOMORKI_GLW AND W.KOD ='D6')
#C
DELETE FROM PR_MAGAZYN M WHERE EXISTS (SELECT NULL FROM PR_KOMORKI K INNER JOIN PR_WYDZIALY W ON W.ID_WYDZIALU = K.ID_WYDZIALU WHERE K.ID_KOMORKI = M.ID_KOMORKI AND W.KOD ='D6')
