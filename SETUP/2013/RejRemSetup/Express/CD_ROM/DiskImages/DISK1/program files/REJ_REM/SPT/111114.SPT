UPDATE PR_OSOBY OS  SET ETAT_KOR =1 WHERE EXISTS
(select NULL from pr_stanowiska st where ST.ID_STANOWISKA =OS.ID_STANOWISKA AND UPPER(st.NAZWA) like '%KIEROWNIK%')
#C

CREATE OR ALTER TRIGGER TR_PR_REMONTY_BI FOR PR_REMONTY
ACTIVE BEFORE  INSERT POSITION 0
AS

begin
 IF(NEW.ID_REMONTU IS NULL OR NEW.ID_REMONTU < 0 ) THEN NEW.ID_REMONTU = GEN_ID(PR_REMONTY_GEN,1);
end
#C