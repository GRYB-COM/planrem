[Eksport]
GLOWNY=0
Tabele=PR_SPRZET;PR_MAGAZYN;PR_ZLECENIA;PR_ZLECENIA1;PR_ZLECENIA_RBH; PR_KOSZTY;
Adresy=slawomir.grybos@groszek.pl;
katalog=P:\PLANREM\EXE\_EXPORT
[Eksport - PR_SPRZET]
Alias= SPRZ
SQL=select * from pr_sprzet where ID_SPRZETU/10000000 = (SELECT MIN(ID_WYDZIALU) FROM PR_WYDZIALY WHERE GLOWNY ='T')
[Eksport - PR_MAGAZYN]
Alias= M
SQL=select m.*, w.GLOWNY from pr_magazyn m inner join pr_komorki k on k.ID_KOMORKI = m.ID_KOMORKI inner join PR_WYDZIALY w on w.ID_WYDZIALU = k.ID_WYDZIALU
Warunek= m.ROK = @ROK# 
Procedura=PR_ADD2MAGAZYN
[Eksport - PR_ZLECENIA]
SQL=SELECT Z.ID_ZLECENIA, Z.SYGN_ZLECENIA, Z.ID_KOD_U, Z.ID_RODZAJ_R, Z.DATA_WYST, Z.DATA_PROP_ZAK, Z.ID_SPRZETU,  Z.PRZESTOJ_OD, Z.DATA_ZDA_KUT, Z.LICZBA_RBH, Z.DATA_ZAK, Z.ID_GR_SPRZ, Z.ID_RODZ_SPRZ, Z.DEPOZYT, Z.STATUS, Z.ID_MAG, Z.ID_HARM, Z.NR_HARM, Z.UWAGI, Z.ZADANIE, Z.ID_JEDNOSTKI, Z.ID_KOMORKI, Z.ROK, Z.ID_KOMORKI_GLW, Z.ROK_MAG, Z.NR_KOL, Z.SYGN_MAG, Z.NR_MAG  FROM PR_ZLECENIA Z INNER JOIN PR_KOMORKI K ON K.ID_KOMORKI = Z.ID_KOMORKI INNER JOIN PR_WYDZIALY W ON W.ID_WYDZIALU = K.ID_WYDZIALU WHERE Z.ROK = @ROK# AND  W.GLOWNY='T'

Alias = Z 
Procedura = PR_ADD2ZLECENIA
[Eksport - PR_ZLECENIA1]
SQL=SELECT Z.ID_ZLECENIA, Z.SYGN_ZLECENIA, Z.ID_KOD_U, Z.ID_RODZAJ_R, Z.DATA_WYST, Z.DATA_PROP_ZAK, Z.ID_SPRZETU,  Z.PRZESTOJ_OD, Z.DATA_ZDA_KUT, Z.LICZBA_RBH, Z.DATA_ZAK, Z.ID_GR_SPRZ, Z.ID_RODZ_SPRZ, Z.DEPOZYT, Z.STATUS, Z.ID_MAG, Z.ID_HARM, Z.NR_HARM, Z.UWAGI, Z.ZADANIE, Z.ID_JEDNOSTKI, Z.ID_KOMORKI, Z.ROK, Z.ID_KOMORKI_GLW, Z.ROK_MAG, Z.NR_KOL, Z.SYGN_MAG, Z.NR_MAG  FROM PR_ZLECENIA Z INNER JOIN PR_KOMORKI K ON K.ID_KOMORKI = Z.ID_KOMORKI INNER JOIN PR_WYDZIALY W ON W.ID_WYDZIALU = K.ID_WYDZIALU WHERE Z.ROK = @ROK#  AND  W.KOD NOT IN ( 'D6','D3') AND Z.NR_KOL >0 and z.ID_KOMORKI_GLW IN (SELECT K1.ID_KOMORKI FROM PR_KOMORKI K1 INNER JOIN PR_WYDZIALY W1 ON W1.ID_WYDZIALU = K1.ID_WYDZIALU WHERE W1.GLOWNY = 'T')

Alias = Z 
Procedura = PR_ADD2ZLECENIA
[Eksport - PR_ZLECENIA_RBH]
Alias = ZR
SQL=SELECT ZR.* FROM PR_ZLECENIA_RBH ZR INNER JOIN PR_ZLECENIA Z  ON Z.ROK = ZR.ROK AND Z.ID_KOMORKI = ZR.ID_KOMORKI AND Z.ID_ZLECENIA = ZR.ID_ZLECENIA INNER JOIN PR_KOMORKI K ON K.ID_KOMORKI = Z.ID_KOMORKI INNER JOIN PR_WYDZIALY W ON W.ID_WYDZIALU = K.ID_WYDZIALU  WHERE Z.ROK = @ROK# AND  W.GLOWNY='T'


[Eksport - PR_KOSZTY]
SQL=SELECT KO.* FROM PR_KOSZTY KO INNER JOIN PR_ZLECENIA Z  ON Z.ROK = KO.ROK AND Z.ID_KOMORKI = KO.ID_KOMORKI AND Z.ID_ZLECENIA = KO.ID_ZLECENIA INNER JOIN PR_KOMORKI K ON K.ID_KOMORKI = Z.ID_KOMORKI INNER JOIN PR_WYDZIALY W ON W.ID_WYDZIALU = K.ID_WYDZIALU  WHERE Z.ROK = @ROK# AND  W.GLOWNY='T'
Alias=KO

